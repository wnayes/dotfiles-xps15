URxvt.depth: 32
URxvt.background: rgba:1111/1111/1111/eeee

URxvt.scrollBar: false

URxvt.font: xft:Inconsolata:Medium:size=11
URxvt.boldFont: xft:Inconsolata:Bold:size=11

URxvt.foreground: #BBBBBB 

*color0:  #2E3436
*color1:  #A40000
*color2:  #4E9A06
*color3:  #C4A000
*color4:  #3465A4
*color5:  #75507B
*color6:  #CE5C00
*color7:  #BABDB9
*color8:  #555753
*color9:  #EF2929
*color10: #8AE234
*color11: #FCE94F
*color12: #729FCF
*color13: #AD7FA8
*color14: #FCAF3E
*color15: #EEEEEC

# Disable default binding for Ctrl+Shift bindings.
URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt.perl-ext-common: font-size

# font-size
URxvt.keysym.C-Up: perl:font-size:increase
URxvt.keysym.C-Down: perl:font-size:decrease
URxvt.keysym.C-S-Up: perl:font-size:incglobal
URxvt.keysym.C-S-Down: perl:font-size:decglobal
